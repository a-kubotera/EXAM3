<div class=" topPage" style="padding:20px;">
  <% if notice.present? %>
  <section class="wow fadeInDownBig" data-wow-duration="1s" data-wow-iteration="1">
    <div class="message message__info"><%= notice %></div>
  </section>
  <% end %>
  <div class="topPage__login">
    <%= render partial: 'login' unless signed_in?   %>
  </div>
</div>
